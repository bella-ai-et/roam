{
  "cli": { "version": ">= 16.0.0", "appVersionSource": "remote" },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "NPM_CONFIG_LEGACY_PEER_DEPS": "true"
      }
    },
    "development-simulator": {
      "developmentClient": true,
      "distribution": "internal",
      "ios": { "simulator": true },
      "env": {
        "NPM_CONFIG_LEGACY_PEER_DEPS": "true"
      }
    },
    "preview": {
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      },
      "env": {
        "NPM_CONFIG_LEGACY_PEER_DEPS": "true",
        "EXPO_PUBLIC_CONVEX_URL": "$EXPO_PUBLIC_CONVEX_URL",
        "EXPO_PUBLIC_CLERK_PUBLISHABLE_KEY": "$EXPO_PUBLIC_CLERK_PUBLISHABLE_KEY",
        "EXPO_PUBLIC_GOOGLE_MAPS_API_KEY": "$EXPO_PUBLIC_GOOGLE_MAPS_API_KEY",
        "EXPO_PUBLIC_REVENUECAT_API_KEY_ANDROID": "$EXPO_PUBLIC_REVENUECAT_API_KEY_ANDROID",
        "EXPO_PUBLIC_REVENUECAT_API_KEY_IOS": "$EXPO_PUBLIC_REVENUECAT_API_KEY_IOS",
        "CONVEX_DEPLOYMENT": "$CONVEX_DEPLOYMENT"
      }
    },
    "production": {
      "autoIncrement": true,
      "env": {
        "NPM_CONFIG_LEGACY_PEER_DEPS": "true",
        "EXPO_PUBLIC_CONVEX_URL": "$EXPO_PUBLIC_CONVEX_URL",
        "EXPO_PUBLIC_CLERK_PUBLISHABLE_KEY": "$EXPO_PUBLIC_CLERK_PUBLISHABLE_KEY",
        "EXPO_PUBLIC_GOOGLE_MAPS_API_KEY": "$EXPO_PUBLIC_GOOGLE_MAPS_API_KEY",
        "EXPO_PUBLIC_REVENUECAT_API_KEY_ANDROID": "$EXPO_PUBLIC_REVENUECAT_API_KEY_ANDROID",
        "EXPO_PUBLIC_REVENUECAT_API_KEY_IOS": "$EXPO_PUBLIC_REVENUECAT_API_KEY_IOS",
        "CONVEX_DEPLOYMENT": "$CONVEX_DEPLOYMENT"
      }
    }
  },
  "submit": { "production": {} }
}
